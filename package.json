{
  "name": "crank",
  "description": "Test automation for integrated SaaS, by Automaton.",
  "version": "0.4.0",
  "author": "Automaton Inc",
  "bin": {
    "crank": "./bin/run"
  },
  "bugs": "https://github.com/the-automaton/crank/issues",
  "dependencies": {
    "@oclif/command": "^1.5.14",
    "@oclif/config": "^1.13.0",
    "@oclif/parser": "^3.8.1",
    "@oclif/plugin-help": "^2.2.0",
    "@oclif/plugin-update": "^1.3.9",
    "async-retry": "^1.2.3",
    "bluebird": "^3.5.5",
    "chalk": "^2.4.2",
    "cli-ux": "^5.2.1",
    "debug": "^4.1.1",
    "google-protobuf": "^3.8.0",
    "grpc": "^1.21.1",
    "inquirer": "^6.3.1",
    "node-forge": "^0.8.4",
    "rxjs": "^6.5.2",
    "tslib": "^1.10.0",
    "yaml": "^1.6.0",
    "yeoman-environment": "^2.3.4",
    "yeoman-generator": "^4.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3.1.1",
    "@types/async-retry": "^1.4.1",
    "@types/bluebird": "^3.5.27",
    "@types/chai": "^4.1.7",
    "@types/debug": "^4.1.4",
    "@types/google-protobuf": "^3.2.7",
    "@types/inquirer": "^6.0.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.9",
    "@types/node-forge": "^0.8.3",
    "@types/protobufjs": "^5.0.31",
    "@types/rimraf": "^2.0.2",
    "@types/yaml": "^1.0.2",
    "@types/yeoman-environment": "^2.3.1",
    "aws-sdk": "^2.488.0",
    "chai": "^4.2.0",
    "globby": "^8.0.2",
    "grpc_tools_node_protoc_ts": "^2.5.3",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.3",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/the-automaton/crank",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "topics": {
      "cog": {
        "description": "Install, uninstall, test, and authenticate Automaton Cogs."
      },
      "help": {
        "description": "Display help for Crank."
      },
      "registry": {
        "description": "List installed Cogs, available steps, etc."
      },
      "update": {
        "description": "Update the Crank CLI."
      }
    },
    "bin": "crank",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-update"
    ],
    "update": {
      "s3": {
        "xz": true,
        "bucket": "crank-cli",
        "host": "https://crank-assets.automatoninc.com"
      }
    },
    "macos": {
      "identifier": "com.automatoninc.crank"
    }
  },
  "repository": "the-automaton/crank",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json && rm -f tsconfig.tsbuildinfo",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && cp -r src/proto lib/proto && cp -r src/assets lib/assets && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "build-proto": "./scripts/build-proto.sh",
    "build-proto-docs": "docker run --rm -v $(pwd)/src/assets/proto:/out -v $(pwd)/src/assets/proto:/proto pseudomuto/protoc-gen-doc --proto_path=/proto --doc_opt=markdown,README.md /proto/cog.proto && sed -i '' -e 's/&lt;/</g' -e 's/&gt;/>/g' -e 's/&#43;/+/g' -e 's/&#34;/\"/g' src/assets/proto/README.md"
  },
  "types": "lib/index.d.ts"
}
